{
    "version": "1.1",
   "manifest_version": 2,
   "name": "abcd",
   
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "background.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://www.youtube.com/*" ] 
   } ],
       "offline_enabled": true,
   "permissions": [ "storage", "webRequest"]
}